%PMB\newbox\pt@box
%PMB\newdimen\pt@width
%PMB\newcount\pt@line
%PMB\newcount\pt@column
%PMB\newcount\pt@nlines
%PMB\newcount\pt@ncol
%PMB\newcount\pt@page
%PMB
%PMB% See 'deluxetable' environment definition for \pt@addcol
%PMB% The def was moved so that \tablecolumns only affects the current table
%PMB\def\colhead#1{\multicolumn{1}{c}{#1}\pt@addcol}
%PMB\def\tablecolumns#1{\pt@column=#1\pt@ncol=#1\gdef\pt@addcol{\relax}}
%PMB\def\tablecaption#1{\gdef\pt@caption{#1}} \def\pt@caption{\relax}
%PMB\def\tablehead#1{\gdef\pt@head{\hline\hline\relax\\[-1.7ex]
%PMB	#1\hskip\tabcolsep\\[.7ex]\hline\relax\\[-1.5ex]}} \def\pt@head{\relax}
%PMB\def\tabletail#1{\gdef\pt@tail{#1}} \def\pt@tail{\relax}
%PMB\def\tablewidth#1{\pt@width=#1} \pt@width\textwidth
%PMB\def\tableheadfrac#1{\gdef\pt@headfrac{#1}} \def\pt@headfrac{.1}
%PMB
%PMB% Calculate the number of tabular rows that will fit in the body of the
%PMB% table.  The height of the table body and the number of lines are
%PMB% calculated thus:
%PMB%
%PMB%     bodyheight = \textheight * (1 - \pt@headfrac)
%PMB%     \pt@nlines = bodyheight / (\arraystretch * \baselineskip)
%PMB%
%PMB% The row height can be seen to be valid algebraically if one examines the
%PMB% definitions for \strutbox, \@setstrut, and \@array.  I could eliminate
%PMB% this calculation, as well as its attendant parameters, if it were possible
%PMB% to determine the accumulated height of the \halign box as TeX goes along.
%PMB
%PMB\def\pt@calcnlines{\@tempdima\pt@headfrac\textheight
%PMB	\@tempdimb\textheight\advance\@tempdimb by-\@tempdima
%PMB	\@tempdima\arraystretch\baselineskip
%PMB	\divide\@tempdimb by\@tempdima
%PMB	\global\pt@nlines\@tempdimb}
