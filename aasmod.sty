%PMB\def\spew@tblnotes{
%PMB  \ifx\tblnote@list\@empty\relax
%PMB  \else
%PMB	\vspace{4.5ex}
%PMB	\footnoterule
%PMB	\vspace{.5ex}
%PMB	{\footnotesize\tblnote@list}
